{
  "cli": {
    "version": ">= 15.0.15",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "environment": "development"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "preview",
      "environment": "preview"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "channel": "production",
      "environment": "production",
      "env": {
        "EXPO_PUBLIC_API_URL": "$EXPO_PUBLIC_API_URL",
        "EXPO_PUBLIC_GITHUB_CLIENT_ID": "$EXPO_PUBLIC_GITHUB_CLIENT_ID",
        "EXPO_PUBLIC_LINKEDIN_CLIENT_ID": "$EXPO_PUBLIC_LINKEDIN_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "$EXPO_PUBLIC_GOOGLE_CLIENT_ID"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
