{
  "expo": {
    "name": "Gamix",
    "slug": "Gamix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "com.gamix",
    "owner": "g4mix",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D8CEFA"
    },
    "ios": {
      "bundleIdentifier": "com.gamix",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.499249588653-dakic31dp5oor0lm5mab40ulivdv5coa"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#D8CEFA"
      },
      "package": "com.gamix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-font",
        {
          "fonts": ["./src/assets/heroicons/heroicons.ttf"]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir ao $(PRODUCT_NAME) o acesso a câmera?",
          "microphonePermission": "Permitir ao $(PRODUCT_NAME) o acesso ao microfone?",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir ao $(PRODUCT_NAME) o acesso aos arquivos de mídia?",
          "savePhotosPermission": "Permitir ao $(PRODUCT_NAME) salvar fotos?",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "Permitir ao $(PRODUCT_NAME) acessar o seu calendário?",
          "remindersPermission": "Permitir ao $(PRODUCT_NAME) acessar os lembretes?"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f85eefe3-3521-4782-86e0-ff4840533e35"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f85eefe3-3521-4782-86e0-ff4840533e35"
    }
  }
}
